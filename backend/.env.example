MAX_UPLOAD_SIZE_MB=100
UPLOAD_DIR=/tmp/scoutgpt_uploads

# CORS
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# Environment
ENVIRONMENT=development
LOG_LEVEL=INFO
